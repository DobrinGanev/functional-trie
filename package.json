  "devDependencies": {
    "coveralls": "^3.0.0",
    "documentation": "^5.0.0",
    "istanbul": "^0.4.1",
    "standard": "^10.0.0",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wanderer/functional-trie.git"
  },
  "bugs": {
    "url": "https://github.com/wanderer/functional-trie/issues"
  },
  "standard": {
    "ignore": ["/benchmark/"]
